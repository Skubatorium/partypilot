# Application URLs
VITE_APP_URL=http://localhost:3000 # Frontend URL (use https:// in production)
VITE_API_URL=http://localhost:3001 # Backend API URL (use https:// in production)

# Environment
VITE_NODE_ENV=development # Options: development, production

# Clerk Authentication
# Get your key from https://dashboard.clerk.com/
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key

# Geoapify API
# Get your key from https://www.geoapify.com/get-started-with-maps-api
VITE_GEOAPIFY_API_KEY=your_geoapify_api_key